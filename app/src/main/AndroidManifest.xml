<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.classificationcovid">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/corona"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/corona"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClassificationCovid"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".web.WebActivity"></activity>
        <activity android:name=".web.Information"></activity>
        <activity android:name=".splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity" />

    </application>

</manifest>